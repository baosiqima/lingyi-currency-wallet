<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="CoinExchange.IdentityAccess.Domain.Model" namespace="CoinExchange.IdentityAccess.Domain.Model.UserAggregate">
  <class name="PasswordCodeRecord" table="PasswordCodeRecord" lazy="false">
    <id name="_id" column="Id">
      <generator class="assigned" />
    </id>
    <property name="ForgotPasswordCode"  column="ForgotPasswordCode"/>
    <property name="ExpirationDateTime"  column="ExpirationDateTime"/>
    <property name="CreationDateTime"  column="CreationDateTime"/>
    <property name="IsUsed"  column="IsUsed"/>
    <property name="UserId"  column="UserId"/>
   </class>
</hibernate-mapping>